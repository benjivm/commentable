{
    "name": "benjivm/commentable",
    "description": "Commentable Polymorphic Eloquent Models for for Laravel",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Commentable",
        "Laravel Commentable"
    ],
    "license": "MPL-2.0",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }, {
        "name": "Ben Miller",
        "email": "benjivm@protonmail.com",
        "homepage": "https://ben-miller.com"
    }],
    "require": {
        "php": ">=8.0",
        "illuminate/support": ">=8.0",
        "illuminate/database": ">=8.0",
        "kalnoy/nestedset": "^6.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Benjivm\\Commentable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Benjivm\\Tests\\Commentable\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Benjivm\\Commentable\\CommentableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
